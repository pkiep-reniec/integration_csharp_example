@model idaas_sdk_csharp.dto.User

<h1 class="text-center">RENIEC IDaaS</h1>
<h4 class="text-center">Auth Result</h4>

<div class="col-sm-12">
    <p><b>DNI:</b> @Model.doc</p>
    <p><b>Primer nombre:</b> @Model.firstName</p>
    <p><b>Teléfono:</b> @Model.phoneNumber</p>
    <p><b>Teléfono verificado:</b> @Model.phoneNumberVerified</p>
    <p><b>Email:</b> @Model.email</p>
    <p><b>Email verificado:</b> @Model.emailVerified</p>
    <p><b>Sub:</b> @Model.sub</p>
</div>

<div class="col-sm-12 text-center">
    <a href="logout" class="btn btn-danger">Salir</a>
</div>